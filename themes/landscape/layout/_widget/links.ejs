<% if (site.data.links){ %>
  <div class="widget-wrap">
    <h3 class="widget-title"><%= __('links') %></h3>
      <div class="widget">
      	<ul>
        	<% for (var i in site.data.links){ %>
          	<li class='link'><a href='<%- site.data.links[i] %>'><%= i %></a></li>
        	<% } %>
      	</ul>
      </div>
  </div>
<% } %>